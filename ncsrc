# bash/sh/zsh environment setup for NSO ncs-5.7.1
NCS_DIR=/Users/jomira/DEVV
PATH=$NCS_DIR/bin:$PATH
if [ "x${DYLD_LIBRARY_PATH}" != "x" ]; then
    DYLD_LIBRARY_PATH=$NCS_DIR/lib:$DYLD_LIBRARY_PATH
else
    DYLD_LIBRARY_PATH=$NCS_DIR/lib
fi
if [ "x$PYTHONPATH" != "x" ]; then
    PYTHONPATH=$NCS_DIR/src/ncs/pyapi:$PYTHONPATH
else
    PYTHONPATH=$NCS_DIR/src/ncs/pyapi
fi
export NCS_DIR PATH DYLD_LIBRARY_PATH PYTHONPATH
